{
  "name": "@hannoeru/eslint-config-monorepo",
  "version": "0.9.4",
  "private": true,
  "packageManager": "pnpm@7.18.1",
  "license": "MIT",
  "author": "hannoeru <me@hanlee.co>",
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "build": "pnpm -r build",
    "stub": "pnpm -r stub",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s -r 0",
    "release": "pnpm bump && pnpm publish",
    "bump": "pnpm dlx bumpp package.json packages/*/package.json -x 'pnpm changelog' --all",
    "publish": "pnpm -r publish --access public --no-git-checks",
    "type-check": "tsc --noEmit"
  },
  "devDependencies": {
    "@hannoeru/eslint-config": "workspace:*",
    "@hannoeru/eslint-plugin": "workspace:*",
    "@types/react": "^18.0.26",
    "bumpp": "^8.2.1",
    "conventional-changelog-cli": "^2.2.2",
    "eslint": "^8.29.0",
    "jiti": "^1.16.0",
    "react": "^18.2.0",
    "typescript": "^4.9.4",
    "zx": "^7.1.1"
  }
}
