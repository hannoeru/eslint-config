{
  "compilerOptions": {
    "module": "ESNext",
    "target": "ESNext",
    "lib": ["ESNext"],
    "esModuleInterop": true,
    "strict": true,
    "strictNullChecks": true,
    "moduleResolution": "Bundler",
    "resolveJsonModule": true,
    "noUnusedParameters": true
  },
  "include": ["src"]
}
